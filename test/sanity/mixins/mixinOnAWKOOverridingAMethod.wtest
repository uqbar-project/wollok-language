/**
  * Scenario: a named object mixed with a mixin
  */
mixin Flies {
  var times = 0
  method fly() {
    times = 1
  }
  method times() = times
}

object pepita mixed with Flies {
  override method fly() {}
}

/**
  * Tests
  */
test "mixin on a named object overriding a method" {
  pepita.fly()
  assert.equals(0, pepita.times())
}
