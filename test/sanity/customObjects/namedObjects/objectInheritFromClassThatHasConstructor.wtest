/**
  * Scenario: a wko inheriting from a custom superclass with a constructor
  */
class Dog {
	const name
	constructor(param) {
		name = param
	}
	method name() = name
}

object lassie inherits Dog("lassie") {}

/**
  * Tests
  */
test "object inherit from class that has constructor" {
	assert.equals("lassie", lassie.name())
}