/**
  * Scenario: a class with a property
  */
class Auto {
  var property kms
}

/**
  * Tests
  */
test "object inheriting from a class - calling a property" {
  var age = 33

  const dodge = object inherits Auto(kms = 2000) {
    method age() {
      return age
    }
  }

  assert.equals(2000, dodge.kms())
}
