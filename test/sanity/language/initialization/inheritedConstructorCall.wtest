/**
  * Scenario: a class with a constructor call delegating to a complex chained message
  */
class Arma {
  const property mm
}

class Unidad {
  var nombre = "Peloton"
}

class UnidadArmada inherits Unidad {
  var property arma
}

object armeria {
  method arco() = new Arma(25)
}

class Arquero inherits UnidadArmada {
}

/**
  * Tests
	*
	* https://github.com/uqbar-project/wollok/issues/1288
  */
test "issue 1288 - delegating to super in a chained message" {
  new Arquero(arma = armeria.arco())
  assert.that(true)
}
